{
	"name": "ene-ref-napi",
	"description": "Turn Buffer instances into \"pointers\"",
	"engines": {
		"node": ">= 10.0"
	},
	"keywords": [
		"native",
		"buffer",
		"extensions",
		"c++",
		"pointer",
		"reference",
		"dereference",
		"type",
		"int",
		"long",
		"float",
		"double",
		"byte",
		"64",
		"napi"
	],
	"version": "0.0.1",
	"license": "MIT",
	"main": "./lib/ref.js",
	"scripts": {
		"docs": "node docs/compile",
		"test": "nyc mocha --expose-gc",
		"install": "ene-node-gyp-build",
		"prebuild": "prebuildify --napi --tag-armv",
		"prepack": "prebuildify-ci download && ([ $(ls prebuilds | wc -l) = '5' ] || (echo 'Some prebuilds are missing'; exit 1))"
	},
	"dependencies": {
		"debug": "^4.1.1",
		"ene-node-addon-api": "github:energypatrikhu/ene-node-addon-api",
		"ene-node-gyp-build": "github:energypatrikhu/ene-node-gyp-build",
		"get-symbol-from-current-process-h": "^1.0.2"
	},
	"devDependencies": {
		"dox": "0.9.0",
		"ene-weak-napi": "github:energypatrikhu/ene-weak-napi",
		"highlight.js": "^10.0.0",
		"jade": "^1.11.0",
		"marked": "^1.0.0",
		"mocha": "^7.1.1",
		"nyc": "^15.0.0",
		"prebuildify": "^3.0.4",
		"prebuildify-ci": "^1.0.5"
	}
}
